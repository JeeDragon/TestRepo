<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>FactualDriver</id>
        <version>1.6.12</version>
        <authors>Sevada Abraamyan, Denis Pavlov,Sergey Maskalik</authors>
        <owners>Factual</owners>
        <projectUrl>https://github.com/Factual/factual-csharp-driver</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>.NET client library for Factual API. Please refer to https://github.com/Factual/factual-csharp-driver for documentation.</description>
