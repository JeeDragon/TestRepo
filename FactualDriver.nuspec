        <releaseNotes>Fix CA2200 warning</releaseNotes>
        <dependencies>
            <dependency id="Newtonsoft.Json" version="6.0.3" />
        </dependencies>
    </metadata>
    <files>
        <file src="FactualDriver\bin\Release\FactualDriver.XML" target="content\FactualDriver.XML" />
        <file src="FactualDriver\bin\Release\FactualDriver.dll" target="lib\FactualDriver.dll" />
    </files>
</package>
